# MESSAGE-TEMPLATE SUBJECT
#: 23806 SUBJECT
msgid "LAMsight Update Message"
msgstr "LAMsight-Update-Benachrichtigung"

# MESSAGE-TEMPLATE BODY
#: 23806 BODY
msgid "<%author%> writes:\n\n<%title%>\nDate: <%date%>\n\n<%content%>\n"
msgstr "<%author%> schreibt:\n\n<%title%>\nDatum: <%date%>\n\n<%content%>\n"

# MESSAGE-TEMPLATE SUBJECT
#: 23813 SUBJECT
msgid "[LAMsight] New comment"
msgstr "[LAMsight] Neuer Kommentar"

# MESSAGE-TEMPLATE BODY
#: 23813 BODY
msgid "You are an editor for a LAMsight survey that has a new comment on a question\n\nComment by <%author%> (<%date%>) for question\n"<%question%>"\n\nComment text:\n\n<%comment%>\n\n\n<%context%>\n\n\n"
msgstr "Sie bearbeiten einen LAMsight-Fragebogen und haben eine neue Anmerkung zu einer Frage\n\nKommentar von <%author%> (<%date%>) zur  Frage\n"<%question%>"\n\nKommentartext:\n\n<%comment%>\n\n\n<%context%>\n\n\n"

# MESSAGE-TEMPLATE SUBJECT
#: 23816 SUBJECT
msgid "[LAMsight] New post for <%topic%>"
msgstr "[LAMsight] Neuer Beitrag zum Thema <%topic%>"

# MESSAGE-TEMPLATE BODY
#: 23816 BODY
msgid "<%author%> writes:\n\n<%content%>\n\n<%original%>\n\nVisit this topic "<%topic%>":\n<%url%>\n\n"
msgstr "<%author%> schreibt:\n\n<%content%>\n\n<%original%>\n\nDieses Thema aufrufen"<%topic%>":\n<%url%>\n\n"

# MESSAGE-TEMPLATE SUBJECT
#: 23818 SUBJECT
msgid "[LAMsight] Forum digest for <%date%>"
msgstr "[LAMsight] Digest/Forumsbeiträge für <%date%>"

# MESSAGE-TEMPLATE BODY
#: 23818 BODY
msgid "<%count%> new messages\n\n<%activity%>"
msgstr "<%count%> neue Nachrichten\n\n<%activity%>"

# MESSAGE-TEMPLATE SUBJECT
