msgstr "Skulle du säga att din menstruation är:"

# QUESTION CHOICES
#: 6102 CHOICES
msgid "Strong"
msgstr "Kraftig"

# QUESTION CHOICES
#: 6102 CHOICES
msgid "Normal"
msgstr "Normal"

# QUESTION CHOICES
#: 6102 CHOICES
msgid "Light"
msgstr "Lindrig"

# QUESTION PROMPT
#: 6103 PROMPT
msgid "Has this become more or less over time?"
msgstr "Har den blivit kraftigare eller lindrigare med tiden?"

# QUESTION CHOICES
#: 6103 CHOICES
msgid "More"
msgstr "Kraftigare"

