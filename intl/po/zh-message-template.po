# MESSAGE-TEMPLATE SUBJECT
#: 23806 SUBJECT
msgid "LAMsight Update Message"
msgstr "LAMsight 更新信息"

# MESSAGE-TEMPLATE BODY
#: 23806 BODY
msgid "<%author%> writes:\n\n<%title%>\nDate: <%date%>\n\n<%content%>\n"
msgstr "<%author%> 撰写：\n\n<%title%>\n日期：<%date%>\n\n<%content%>\n"

# MESSAGE-TEMPLATE SUBJECT
#: 23813 SUBJECT
msgid "[LAMsight] New comment"
msgstr "[LAMsight] 新评论"

# MESSAGE-TEMPLATE BODY
#: 23813 BODY
msgid "You are an editor for a LAMsight survey that has a new comment on a question\n\nComment by <%author%> (<%date%>) for question\n"<%question%>"\n\nComment text:\n\n<%comment%>\n\n\n<%context%>\n\n\n"
msgstr "您是一份 LAMsight 问卷的新编辑者，该问卷中的一个问题得到了一条新评论\n\n评论者 <%author%> (<%date%>) 针对问题\n"<%question%>"\n\n评论正文：\n\n<%comment%>\n\n\n<%context%>\n\n\n"

# MESSAGE-TEMPLATE SUBJECT
#: 23816 SUBJECT
msgid "[LAMsight] New post for <%topic%>"
msgstr "[LAMsight] 关于 <%topic%> 的新帖"

# MESSAGE-TEMPLATE BODY
#: 23816 BODY
msgid "<%author%> writes:\n\n<%content%>\n\n<%original%>\n\nVisit this topic "<%topic%>":\n<%url%>\n\n"
msgstr "<%author%> 撰写：\n\n<%content%>\n\n<%original%>\n\n访问该主题 "<%topic%>"：\n<%url%>\n\n"

# MESSAGE-TEMPLATE SUBJECT
