msgstr "Fuertes"

# QUESTION CHOICES
#: 6102 CHOICES
msgid "Normal"
msgstr "Normal"

# QUESTION CHOICES
#: 6102 CHOICES
msgid "Light"
msgstr "Llevaderas"

